//斐波那契数列
------------------------------------------------------------------------------------------------------------------------------------------------------
#include <iostream>
#include <cstring>

const int maxn=(int)1e4+7;
int dp[maxn];
int res[2][2],tmp[2][2]={{1,0},{0,1}};

//递归写法（记忆化搜索）
int F(int n){
    if(n==1||n==2)return 1;
    if(dp[n]!=-1)return dp[n];
    else{
        dp[n]=F(n-1)+F(n-2);
        dp[n]%=10007;
        return dp[n];
    }
}

int main(void){
    int n;
    scanf("%d",&n);
//递推写法
//    int f1=1,f2=1;
//    for(int i=3;i<=n;i++){
//        f2=f1+f2;
//        f1=f2-f1;
//        f2%=10007;
//    }
    memset(dp, -1, sizeof(dp));
    printf("%d\n",F(n));
    return 0;
}

